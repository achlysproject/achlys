[boot]
image_path = /media/mmcsd-0-0/{{release_name}}/erts-{{erts_vsn}}/bin/beam.bin

[erlang]
args = erl.rtems -- +MHas aobf +MMmcs 10 +MMamcbf 1024 +MHlmbcs 128 -mode embedded -home . -pa . -root {{release_name}} -config {{release_name}}/releases/{{release_version}}/sys.config -boot {{release_name}}/releases/{{release_version}}/{{release_name}} -internal_epmd epmd_sup -kernel inetrc "./erl_inetrc" -sname {{release_name}} -setcookie MyCookie

[network]
wlan=enable
ip_self=169.254.16.1
wlan_ip_netmask=255.255.0.0
wlan_mode=adhoc
wlan_adhocname=edge
wlan_channel=6
hostname=my_grisp_board_1
